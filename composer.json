{
    "name": "code4interactive/forms",
    "description": "Laravel Forms Package",
    "homepage": "https://github.com/code4interactive/forms",
    "license": "MIT",
    "keywords": [
        "menu",
        "laravel"
    ],
    "authors": [
        {
            "name": "Artur Bartczak",
            "email": "artur.bartczak@code4.pl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/code4interactive/view"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "illuminate/support" : "~5.0",
        "illuminate/contracts" : "~5.0",
        "illuminate/filesystem" : "~5.0",
        "symfony/yaml": "2.7.*",
        "laravelcollective/html": "5.1.*",
        "code4interactive/view": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Code4\\Forms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code4\\Forms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
